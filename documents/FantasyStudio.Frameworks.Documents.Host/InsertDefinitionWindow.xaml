<Window x:Class="FantasyStudio.Frameworks.Documents.Host.InsertDefinitionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FantasyStudio.Frameworks.Documents.Host"
        xmlns:xaml2="https://github.com/Acorisoft/FantasyStudio/Xaml"
        mc:Ignorable="d"
        Title="InsertDefinitionWindow" Height="450" Width="800">
    <Window.DataContext>
        <local:AppViewModel/>
    </Window.DataContext>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="32" />
        </Grid.RowDefinitions>
        <ListBox SelectionChanged="ListBox_Selected" DisplayMemberPath="Name" ItemsSource="{Binding Generations,Mode=OneWay}">
        </ListBox>
        <StackPanel Grid.Row="1" Orientation="Horizontal">
            <Button Content="Ok" Click="Button_Click"/>
            <Button Content="Cancel" Click="Button_Click_1"/>
        </StackPanel>
    </Grid>
</Window>
