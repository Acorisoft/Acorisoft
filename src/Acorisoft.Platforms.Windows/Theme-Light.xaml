<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- 主题色 -->
    <SolidColorBrush x:Key="App.Accent.Light">#14B6DB</SolidColorBrush>
    <SolidColorBrush x:Key="App.Accent">#2196F3</SolidColorBrush>
    <SolidColorBrush x:Key="App.Accent.Dark">#1976D2</SolidColorBrush>

    <!-- 背景色 -->
    <SolidColorBrush x:Key="App.Background">#FFFFFF</SolidColorBrush>

    <!-- 孤岛按钮:Normal -->
    <!-- 透明按钮:Hover -->
    <SolidColorBrush x:Key="App.Island.Light">#F3F5F7</SolidColorBrush>

    <!-- 孤岛按钮:Hover -->
    <!-- 透明按钮:Pressed -->
    <SolidColorBrush x:Key="App.Island">#D9DCDE</SolidColorBrush>

    <!-- 孤岛按钮:Pressed -->
    <SolidColorBrush x:Key="App.Island.Dark">#B4B6B8</SolidColorBrush>

    <!-- 字体 -->
    <SolidColorBrush x:Key="App.Text.Primary">#131414</SolidColorBrush>
    <SolidColorBrush x:Key="App.Text.Secondary">#373838</SolidColorBrush>
    <SolidColorBrush x:Key="App.Text.Thirdary">#757778</SolidColorBrush>
</ResourceDictionary>