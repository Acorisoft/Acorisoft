<spa:SpaWindow x:Class="Acorisoft.Studio.MainWindow"
               xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
               xmlns:local="clr-namespace:Acorisoft.Studio"
               xmlns:spa="https://github.com/Acorisoft/Spa"
               xmlns:rx="http://reactiveui.net"
               mc:Ignorable="d"
               Title="MainWindow" Height="450" Width="800">
    <Grid>
        <rx:RoutedViewHost Router="{Binding Source={x:Static spa:Spa.Router},Mode=OneWay}"/>
    </Grid>
</spa:SpaWindow>
