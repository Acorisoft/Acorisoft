    <Color x:Key="Colors.MaterialDesign.Green">#4CAF50</Color>
    <Color x:Key="Colors.MaterialDesign.Green.Dark">#388E3C</Color>
    <Color x:Key="Colors.MaterialDesign.LightGreen">#8BC34A</Color>
