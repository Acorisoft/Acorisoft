    <Color x:Key="Colors.MaterialDesign.DeepPurple.Dark">#512DA8</Color>
    <Color x:Key="Colors.MaterialDesign.Indigo">#3F51B5</Color>
    <Color x:Key="Colors.MaterialDesign.Indigo.Dark">#303F9F</Color>
    <Color x:Key="Colors.MaterialDesign.Blue">#2196F3</Color>
    <Color x:Key="Colors.MaterialDesign.Blue.Dark">#1976D2</Color>
    <Color x:Key="Colors.MaterialDesign.LightBlue">#03A9F4</Color>
    <Color x:Key="Colors.MaterialDesign.LightBlue.Dark">#0288D1</Color>
    <Color x:Key="Colors.MaterialDesign.Cyan">#00BCD4</Color>
    <Color x:Key="Colors.MaterialDesign.Cyan.Dark">#0097A7</Color>
    <Color x:Key="Colors.MaterialDesign.Teal">#009688</Color>
