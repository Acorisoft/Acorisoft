    <SolidColorBrush x:Key="Brushes.MaterialDesign.DeepOrange.Dark" Color="{StaticResource Colors.MaterialDesign.DeepOrange.Dark}"/>

    <!-- 常用画刷 -->
    <SolidColorBrush x:Key="Brushes.Black" Color="{StaticResource Colors.Black}"/>
    <SolidColorBrush x:Key="Brushes.White" Color="{StaticResource Colors.White}"/>
    <SolidColorBrush x:Key="Brushes.Paper" Color="{StaticResource Colors.Paper}"/>
    <SolidColorBrush x:Key="Brushes.Accent" Color="{StaticResource Colors.Accent}"/>
    <SolidColorBrush x:Key="Brushes.Accent.Dark" Color="{StaticResource Colors.Accent.Dark}"/>
    <SolidColorBrush x:Key="Brushes.Transparent" Color="Transparent"/>
    <SolidColorBrush x:Key="Brushes.Mask.Dark">#30000000</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.Mask.Light">#30FFFFFF</SolidColorBrush>

    <!-- 阴影 -->
    <DropShadowEffect x:Key="Effect.Shadow1" BlurRadius="5" ShadowDepth="1" Direction="270" Color="{StaticResource Colors.Shadow}" Opacity=".2" RenderingBias="Performance" o:Freeze="True" />
    <DropShadowEffect x:Key="Effect.Shadow2" BlurRadius="8" ShadowDepth="1.5" Direction="270" Color="{StaticResource Colors.Shadow}" Opacity=".2" RenderingBias="Performance" o:Freeze="True" />
    <DropShadowEffect x:Key="Effect.Shadow3" BlurRadius="14" ShadowDepth="4.5" Direction="270" Color="{StaticResource Colors.Shadow}" Opacity=".2" RenderingBias="Performance" o:Freeze="True" />
    <DropShadowEffect x:Key="Effect.Shadow4" BlurRadius="25" ShadowDepth="8" Direction="270" Color="{StaticResource Colors.Shadow}" Opacity=".2" RenderingBias="Performance" o:Freeze="True" />
    <DropShadowEffect x:Key="Effect.Shadow5" BlurRadius="35" ShadowDepth="13" Direction="270" Color="{StaticResource Colors.Shadow}" Opacity=".2" RenderingBias="Performance" o:Freeze="True" />

    <SolidColorBrush x:Key="Brushes.Commons.Background" Color="{StaticResource Colors.Transparent}"/>
    <SolidColorBrush x:Key="Brushes.Commons.Background.Hover">#30000000</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.Commons.Background.Press">#50000000</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.Commons.Foreground" Color="{StaticResource Colors.Black}"/>
    <SolidColorBrush x:Key="Brushes.Commons.Foreground.Hover" Color="{StaticResource Colors.White}"/>
    <SolidColorBrush x:Key="Brushes.Commons.Foreground.Press" Color="{StaticResource Colors.White}"/>


    <SolidColorBrush x:Key="App.Dark.Mask">#90000000</SolidColorBrush>
    <SolidColorBrush x:Key="App.Dark.Lighten.Mask">#30000000</SolidColorBrush>
    <SolidColorBrush x:Key="App.White.Lighten.Mask">#50FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="App.White.Darken.Mask">#90FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="App.Text.Primary">#3c4858</SolidColorBrush>
    <SolidColorBrush x:Key="App.Text.Secondary">#8492a6</SolidColorBrush>
    <SolidColorBrush x:Key="App.Theme.Primary" Color="{StaticResource Colors.MaterialDesign.LightGreen}" />
    <SolidColorBrush x:Key="App.Theme.Secondary" Color="{StaticResource Colors.MaterialDesign.LightGreen.Dark}" />

</ResourceDictionary> 