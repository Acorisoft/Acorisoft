<rx:ReactiveUserControl x:Class="Acorisoft.Morisa.Dialogs.NotificationView"
                        x:TypeArguments="dialog:Notification"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                        xmlns:dialog="clr-namespace:Acorisoft.Morisa.Dialogs"
                        xmlns:rx="http://reactiveui.net"
                        mc:Ignorable="d" 
                        d:DesignHeight="450" d:DesignWidth="800"
                        Width="384"
                        Height="256"
                        d:DataContext="{d:DesignInstance IsDesignTimeCreatable=True,Type={x:Type dialog:Notification}}">
    <Border CornerRadius="8"
            Background="White">
        <DockPanel>
            <Button Content="Ok" Command="{x:Static dialog:DialogCommands.Ok}"/>
        </DockPanel>
    </Border>
</rx:ReactiveUserControl>