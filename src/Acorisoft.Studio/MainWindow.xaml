<Window x:Class="Acorisoft.Studio.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Acorisoft.Studio"
        xmlns:w="https://github.com/Acorisoft/Acorisoft/Windows"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <w:BusyIndicator x:Name="Indicator">
            <w:ToastHost x:Name="Toast">
                <w:DialogHost x:Name="Dialog">
                    <Grid>
                    </Grid>
                </w:DialogHost>
            </w:ToastHost>
        </w:BusyIndicator>
    </Grid>
</Window>
