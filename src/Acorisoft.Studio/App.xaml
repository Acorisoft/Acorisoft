<Application x:Class="Acorisoft.Studio.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:app="clr-namespace:Acorisoft.Studio"
                    xmlns:w="https://github.com/Acorisoft/Acorisoft/Windows"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Acorisoft.Extensions.Platforms.Windows;component/Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <Style TargetType="w:SpaWindow" BasedOn="{StaticResource Windows}"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
